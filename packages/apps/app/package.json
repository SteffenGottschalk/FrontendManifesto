{
  "name": "@project/app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "lint": "eslint \"src/**/*.{js,jsx}\" --fix --max-warnings 5"
  },
  "dependencies": {
    "@project/basic-components": "*",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "web-vitals": "1.0.1"
  },
  "devDependencies": {
    "@project/eslint-config": "*",
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.1.0",
    "@testing-library/user-event": "12.1.10",
    "customize-cra": "1.0.0",
    "react-app-rewire-hot-loader-for-customize-cra": "1.0.1",
    "react-app-rewired": "2.1.8",
    "react-hot-loader": "4.13.0",
    "react-scripts": "4.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "!src/reportWebVitals.js",
      "!src/**/index.js",
      "!src/**/*.(spec|test|stories).{js}",
      "!src/**/*.svg"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 98,
        "branches": 98,
        "functions": 98,
        "lines": 98
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
